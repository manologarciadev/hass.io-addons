{
    "name": "Mopidy",
    "version": "0.1.1-3",
    "slug": "mopidy",
    "description": "mopidy music server",
    "url": "https://github.com/vmga43/hassio-addons/tree/master/mopidy",
    "startup": "application",
    "boot": "auto",
    "image": "vmga43/{arch}-mopidy",
    "options": {"local_scan": true},
    "schema": {
